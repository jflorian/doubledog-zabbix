{
    "name": "doubledog-zabbix",
    "version": "1.1.0",
    "author": "John Florian",
    "license": "GPL-3.0",
    "summary": "A module to manage Zabbix servers and monitored hosts",
    "source": "https://www.doubledog.org/git/doubledog-zabbix.git",
    "operatingsystem_support": [
        {
            "operatingsystem": "CentOS",
            "operatingsystemmajrelease": [
                "7",
                "8"
            ]
        },
        {
            "operatingsystem": "Fedora",
            "operatingsystemrelease": [
                "29",
                "30",
                "31"
            ]
        }
    ],
    "dependencies": [
        {
            "name": "doubledog/ddolib",
            "version_requirement": ">= 1.0.0 < 2.0.0"
        },
        {
            "name": "puppetlabs/stdlib",
            "version_requirement": ">= 3.2.0 < 6.0.0"
        },
        {
            "name": "puppetlabs/firewall",
            "version_requirement": ">= 1.8.0 < 3.0.0"
        }
    ]
}
