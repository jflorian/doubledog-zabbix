{
    "name": "doubledog-zabbix",
    "version": "0.1.0",
    "author": "John Florian",
    "license": "GPL-3.0",
    "summary": "A module to manage Zabbix servers and monitored hosts",
    "source": "https://www.doubledog.org/git/doubledog-zabbix.git",
    "operatingsystem_support": [
        {
            "operatingsystem": "CentOS",
            "operatingsystemrelease": [
                "7.0"
            ]
        },
        {
            "operatingsystem": "Fedora",
            "operatingsystemrelease": [
                "23",
                "24",
                "25"
            ]
        }
    ],
    "dependencies": [
        {
            "name": "puppetlabs/stdlib",
            "version_requirement": ">= 3.2.0 < 5.0.0"
        },
        {
            "name": "puppetlabs/firewall",
            "version_requirement": ">= 0.0.4 < 2.0.0"
        }
    ]
}
